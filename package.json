        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@glidejs/glide": "^3.4.1",
        "baguettebox.js": "^1.11.0",
        "bigpicture": "^2.3.1",
        "dayjs": "^1.8.20",
